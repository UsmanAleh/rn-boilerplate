{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web:development": "NODE_ENV=development webpack serve --config webpack.config.js --open",
    "web:staging": "NODE_ENV=staging webpack serve --config webpack.config.js --open",
    "web:production": "NODE_ENV=production webpack serve --config webpack.config.js --open",
    "start": "react-native start",
    "clean": "rm -rf node_modules dist build && npm cache clean --force",
    "typechain:build": "typechain --target=ethers-v6 --out-dir src/contracts/typechain src/contracts/abis/*.json",
    "test": "jest",
    "lint:rules": "eslint . --cache",
    "lint:code-format": "prettier --check \"{src,__mocks__}/**/*.{js,json,md,ts,tsx,yml,yaml}\"",
    "lint:type-check": "tsc",
    "lint": "yarn lint:rules && yarn lint:code-format && yarn lint:type-check",
    "lint:fix": "yarn lint:rules --fix && yarn lint:code-format --write && yarn lint:type-check",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "npx pod-install",
    "postinstall": "yarn typechain:build && npx patch-package"
  },
  "dependencies": {
    "@ethersproject/shims": "^5.7.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-native/assets-registry": "^0.76.5",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.4.0",
    "ethers": "^6.13.4",
    "ky": "^1.7.2",
    "process": "^0.11.10",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-native": "0.76.1",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.15.4",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "4.0.0",
    "react-native-svg": "^15.7.1",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.19.13",
    "react-native-web-linear-gradient": "^1.1.2",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^5.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.25.9",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@react-native-community/cli": "15.0.0",
    "@react-native-community/cli-platform-android": "15.0.0",
    "@react-native-community/cli-platform-ios": "15.0.0",
    "@react-native/babel-preset": "0.76.1",
    "@react-native/eslint-config": "0.76.1",
    "@react-native/metro-config": "0.76.1",
    "@react-native/typescript-config": "0.76.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/react-native": "^12.1.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/react": "^18.2.6",
    "@types/react-native-web": "^0.19.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^9.2.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-native-web": "^0.19.13",
    "babel-plugin-root-import": "^6.6.0",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "ts-loader": "^9.5.1",
    "typechain": "^8.3.2",
    "typescript": "5.6.3",
    "typescript-eslint": "^7.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
